{
  "buildCommand": false,
  "outputDirectory": ".",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/s/:id",
      "destination": "/verify/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}